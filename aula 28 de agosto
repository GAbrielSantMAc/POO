double[] valores = new double[5];
double somaValores;
double mediaAritmetica;
for (int i = 0; i < valores.Length; i++)
{
    Console.WriteLine($"Informe {i + 1} valor");
    valores[i] = Convert.ToDouble(Console.ReadLine());
    somaValores[] += valores[i];

}
    mediaAritmetica = somaValores / valores.Length;
    Console.WriteLine($"Sua média aritmetica é {mediaAritmetica}");
    
